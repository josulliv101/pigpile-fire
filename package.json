{
  "name": "pigpile-fire",
  "private": true,
  "version": "0.0.0",
  "description": "pigpile-fire",
  "main": "index.js",
  "engines": {
    "node": "6.11.x",
    "npm": "3.10.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josulliv101/pigpile-fire.git"
  },
  "author": "Joe Sullivan",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/josulliv101/pigpile-fire/issues"
  },
  "homepage": "https://github.com/josulliv101/pigpile-fire#readme",
  "scripts": {
    "buildClient": "./node_modules/.bin/webpack --config ./webpack/webpack.client.config.js",
    "buildServer": "./node_modules/.bin/webpack --config ./webpack/webpack.server.config.js",
    "buildServerDev": "./node_modules/.bin/webpack --config ./webpack/webpack.server.config.dev.js",
    "build": "yarn run buildClient && yarn run buildServer",
    "build:server:dev": "yarn run buildServerDev",
    "start:client:dev": "./node_modules/.bin/webpack-dev-server  --config ./webpack/webpack.client.config.dev.js"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "firebase": "^4.5.0",
    "material-ui": "next",
    "material-ui-icons": "1.0.0-beta.15",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "next",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.25.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-form": "^7.1.0",
    "redux-saga": "^0.15.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "webpack": "2.5.0",
    "webpack-dev-server": "^2.9.1"
  },
  "babel": {
    "presets": [
      "react",
      "stage-0"
    ]
  }
}
